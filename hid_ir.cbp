<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="hid_ir" />
		<Option pch_mode="2" />
		<Option compiler="arm_none_eabi_gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/hid_ir" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="arm_none_eabi_gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-O0" />
					<Add directory="../libopencm3/include/libopencm3/stm32" />
					<Add directory="../libopencm3/include/libopencmsis/stm32" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/hid_ir" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="arm_none_eabi_gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after="arm-none-eabi-strip ${TARGET_OUTPUT_FILE}" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-mcpu=cortex-m3" />
			<Add option="-mthumb" />
			<Add option="-Wall" />
			<Add option="-std=c99" />
			<Add option="-DSTM32F1" />
			<Add directory="../arm_common/libopencm3/include" />
		</Compiler>
		<Linker>
			<Add option="-T../arm_common/stm32f103.ld" />
			<Add option="-mcpu=cortex-m3" />
			<Add option='-Wl,-Map,&quot;${TARGET_OUTPUT_DIR}/${TARGET_OUTPUT_BASENAME}.map&quot;' />
			<Add option="-Wl,--print-memory-usage" />
			<Add library="libopencm3_stm32f1.a" />
			<Add directory="../arm_common/libopencm3/lib" />
		</Linker>
		<ExtraCommands>
			<Add after="#arm-none-eabi-objcopy -O binary ${TARGET_OUTPUT_DIR}${TARGET_OUTPUT_BASENAME} ${TARGET_OUTPUT_DIR}${TARGET_OUTPUT_BASENAME}.bin" />
			<Add after="#arm-none-eabi-objcopy -O ihex ${TARGET_OUTPUT_DIR}${TARGET_OUTPUT_BASENAME} ${TARGET_OUTPUT_DIR}${TARGET_OUTPUT_BASENAME}.hex" />
		</ExtraCommands>
		<Unit filename="hid_functions.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="hid_functions.h">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="mouse_actions.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="mouse_actions.h" />
		<Unit filename="nec_infrared.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="nec_infrared.h" />
		<Extensions>
			<debugger>
				<remote_debugging>
					<options conn_type="0" ip_address="localhost" ip_port="3333" additional_cmds="monitor reset halt&#x0A;load" />
				</remote_debugging>
			</debugger>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
